{
  "presets": [
    [
      "@babel/preset-env", {
        "useBuiltIns": "entry",
        "targets": {
          "browsers": ["last 2 versions", "safari >= 7","ie >= 7"],
          "node": "current"
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["import",{
      "libraryName":"antd",
      "libraryDirectory": "es",
      "style": true
    }]
  ]
}